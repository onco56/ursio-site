---
import BaseLayout from "../layouts/BaseLayout.astro";
---

<BaseLayout>
  <fragment slot="head">
    <title>URSIO – Seite nicht gefunden</title>
    <meta
      name="description"
      content="Die angeforderte Seite existiert nicht oder wurde verschoben. Dies ist die 404-Fehlerseite von URSIO."
    />
    <link rel="canonical" href="https://ursio.ch/404" />
    <meta property="og:title" content="URSIO – Seite nicht gefunden" />
    <meta
      property="og:description"
      content="Die von dir gesuchte Seite konnte nicht gefunden werden."
    />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://ursio.ch/404" />
  </fragment>

  <section class="not-found-page">

    <header>
      <h1>404 – Seite nicht gefunden</h1>
    </header>

    <article>
      <p>Die angeforderte Seite existiert nicht.</p>
    </article>

  </section>
</BaseLayout>

